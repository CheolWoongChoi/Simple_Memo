{
	"compilerOptions": {
		"strict": true,
		"lib": [
			"ES5",
			"ES2015",
			"ES2016",
			"ES2017",
			"DOM"
		],
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"src": ["src"],
			"src/*": ["src/*"],
			"public/*": ["public/*"],
			"images/*": ["public/images/*"],
			"pages/*": ["src/pages"],
			"constants": ["src/constants"],
			"constants/*": ["src/constants/*"],
			"components/*": ["src/components/*"],
			"store": ["src/store"],
			"store/*": ["src/store/*"],
		}
	}
}